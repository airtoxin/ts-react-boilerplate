{
  "name": "ts-react-boilerplate",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "clean": "rimraf dist",
    "build": "run-s clean build:transpile",
    "build:transpile": "webpack --mode production",
    "fmt": "prettier --write '**/*.{json,js,jsx,ts,tsx,md}' --ignore-path .gitignore",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "run-p test:*",
    "test:compile": "tsc --noEmit",
    "test:fmt": "prettier --list-different '**/*.{json,js,jsx,ts,tsx,md}' --ignore-path .gitignore",
    "test:lint": "tslint --project tsconfig.json",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@types/history": "4.7.2",
    "@types/jest": "24.0.6",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "7.0.1",
    "@types/react-router": "4.4.4",
    "@types/react-router-dom": "4.3.1",
    "autodll-webpack-plugin": "0.4.2",
    "copy-webpack-plugin": "4.6.0",
    "html-webpack-plugin": "3.2.0",
    "inline-environment-variables-webpack-plugin": "1.2.1",
    "jest": "24.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "ts-loader": "5.3.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.0"
  },
  "dependencies": {
    "connected-react-router": "6.3.1",
    "history": "4.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-redux": "6.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.85.0",
    "typestyle": "2.0.1"
  }
}
